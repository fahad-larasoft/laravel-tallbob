{
  "name": "fahad-larasoft/tallbob",
  "description": "It handles Tall Bob SMS API via simple Facade",
  "license": "MIT",
  "keywords": [
    "Laravel Tallbob",
    "TallBob",
    "Tall Bob",
    "sms",
    "PHP Tallbob",
    "Fahad Ali"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Fahad Ali",
      "email": "fahad@larasoft.io"
    }
  ],
  "require": {
    "guzzlehttp/guzzle": "^7.0.1",
    "illuminate/support": "^8.0|^9.0|^10.0",
    "php": "~7.0|^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "~3.7.0",
    "phpunit/phpunit": "^7.0"
  },
  "autoload": {
    "psr-4": {
      "Larasoft\\TallBobSms\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Larasoft\\VidCorpSms\\Tests\\": "tests"
    },
    "files": [
      "vendor/phpunit/phpunit/src/Framework/Assert/Functions.php"
    ]
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Larasoft\\TallBobSms\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
